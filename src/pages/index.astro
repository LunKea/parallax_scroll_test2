---
import MainLayout from "../layouts/MainLayout.astro";
import Button from "../components/Button.astro";
import Link from "../components/Link.astro";
// import { Image } from "astro:assets";
// import thumbsup from "../assets/images/thumbsup.png";
---

<MainLayout title="Titel">
  <section class="flex flex-row gap-10 m-50 mt-450">
    <section class="">
      <div
        class="bg-red-500 w-10 h-10 text-center place-content-center a opacity-0"
      >
        <p>a</p>
      </div>
    </section>

    <section class="">
      <div
        class="bg-green-500 w-10 h-10 text-center place-content-center b opacity-0"
      >
        <p>b</p>
      </div>
    </section>

    <section class="">
      <div
        class="bg-blue-500 w-10 h-10 text-center place-content-center c opacity-0"
      >
        <p>c</p>
      </div>
    </section>
  </section>

  <script is:inline type="module" src="/src/pages/script.js"></script>
</MainLayout>
